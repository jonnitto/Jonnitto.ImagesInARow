{
    "name": "jonnitto/imagesinarow",
    "type": "neos-plugin",
    "license": "GPL-3.0-or-later",
    "description": "Put images nicley in a row",
    "keywords": ["flow", "neos", "content", "row", "images"],
    "authors": [
        {
            "name": "Jon Uhlmann",
            "email": "me@jonnitto.ch",
            "homepage": "https://twitter.com/jonnitto",
            "role": "Developer"
        }
    ],
    "require": {
        "neos/neos": "~4.3 || ~5.0 || ~7.0",
        "flowpack/nodetemplates": "^1.0",
        "carbon/eel": "^1.7 || dev-master",
        "carbon/image": "^3.0 || dev-master",
        "carbon/includeassets": "^3.5 || ^4.0 || ^5.0 || dev-master"
    },
    "archive": {
        "exclude": [
            ".editorconfig",
            ".gitattributes",
            ".gitignore",
            ".github",
            ".nvmrc",
            ".prettierignore",
            ".stylelintrc",
            ".yarnclean",
            "CODE_OF_CONDUCT.md",
            "package.json"
        ]
    },
    "autoload": {
        "psr-4": {
            "Jonnitto\\ImagesInARow\\": "Classes"
        }
    },
    "extra": {
        "marketplace-account": "me@jonnitto.ch",
        "installer-name": "Jonnitto.ImagesInARow",
        "neos": {
            "package-key": "Jonnitto.ImagesInARow"
        }
    }
}
